    "test:playwright:ui": "playwright test --ui"
  },
  "lint-staged": {
    "**/*.(ts|tsx|js)": [
      "eslint --fix",
      "prettier --cache --write"
    ],
    "*.{css,json}": "prettier --cache --write"
  },
  "browserslist": [
