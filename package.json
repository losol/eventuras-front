  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "next dev",
    "fix-all": "npm run lint-fix && npm run prettier-fix",
    "lint": "next lint",
    "lint-fix": "eslint --fix --ext .ts,.tsx .",
    "prepare": "husky install",
    "prettier-fix": "prettier . --write",
    "start": "next start",
